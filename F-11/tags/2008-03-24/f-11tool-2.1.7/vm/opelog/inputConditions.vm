#errorMarkup()

<h3>$head_title</h3>
<form method="POST" action="$link.setAction('/downloadOpeLog')" >
  <table border="0" cellpadding="5">
  <tr>
    <td>日付指定</td>
    <td><input type="checkbox" name="stEneble" #if ($opelogForm.stEneble == true) checked #end />開始日付
    <input type="text" name="stYear" value="$!opelogForm.stYear" size="5" />年
    #selectmonth( "stMonth" $opelogForm.stMonth )月
    #selectdate( "stDay" $opelogForm.stDay )日
    #selecthour( "stHour" $opelogForm.stHour )時
    #select60( "stMinute" $opelogForm.stMinute )分
    #select60( "stSecond" $opelogForm.stSecond )秒
    </td>
  </tr>
  <tr>
    <td><br></td>
    <td><input type="checkbox" name="etEneble" #if ($opelogForm.etEneble == true) checked #end />終了日付
    <input type="text" name="etYear" value="$!opelogForm.etYear" size="5" />年
    #selectmonth( "etMonth" $opelogForm.etMonth )月
    #selectdate( "etDay" $opelogForm.etDay )日
    #selecthour( "etHour" $opelogForm.etHour )時
    #select60( "etMinute" $opelogForm.etMinute )分
    #select60( "etSecond" $opelogForm.etSecond )秒
    </td>
  </tr>
  <tr>
    <td>ユーザー</td>
    <td>
    <input type="text" name="opeUser" value="$!opelogForm.opeUser" />
    </td>
  </tr>
  <tr>
    <td>IP</td>
    <td>
    <input type="text" name="opeIp" value="$!opelogForm.opeIp" />
    </td>
  </tr>
  <tr>
    <td>ポイント名称</td>
    <td>
    <input type="text" name="opeName" value="$!opelogForm.opeName" />
    </td>
  </tr>
  <tr>
    <td>ポイント詳細</td>
    <td>
    <input type="text" name="opeMessage" value="$!opelogForm.opeMessage" />
    </td>
  </tr>
  <tr align=center><td colspan=4 >
  <input type="submit" name="submit" value="ダウンロード"/>
  </td></tr>
  </table>
</form>
