#set( $head_title = "電子メール送信グループ 属性別設定" )
#set( $layout = "Default.vm" )
#if ( $user.hasPermission("email_attribute_setting") )

#errorMarkup()

<h3>$head_title</h3>
属性別リスト
<table border="0" cellpadding="5">
  <tr>
    <td bgcolor="Skyblue">ID</td>
    <td bgcolor="Skyblue">属性名</td>
    <td bgcolor="Skyblue">送信グループID,メールアドレス</td>
    <td bgcolor="Skyblue"></td>
  </tr>
  #foreach ($emailgroupAttribute in $emailgroupAttributeList)
  <tr>
    <td bgcolor="Khaki" align="right">$!emailgroupAttribute.attribute_id</td>
    <td bgcolor="Khaki">$!F11util.htmlEscape($emailgroupAttribute.attribute_name)</td>
    <td bgcolor="Khaki">$!F11util.htmlEscape($emailgroupAttribute.email_address)</td>
    <td><a href="$link.setForward('editEmailgroupAttribute').addQueryData('attribute_id',"$emailgroupAttribute.attribute_id")">変更</a></td>
  </tr>
  #end
</table>
#end
